<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.g6ix.search_developer">

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Search Developer"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DevActivities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.gory.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity
        android:name=".DevActivities.DevProfile"
        android:parentActivityName=".DevActivities.MainActivity"
        android:theme="@style/AppTheme.NoActionBar" />

    <activity android:name=".DevActivities.SettingsActivity" />
    </application>

</manifest>